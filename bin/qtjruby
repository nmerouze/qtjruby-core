#!/usr/bin/env jruby
require 'rubygems'

if File.exist?(gem_dir = File.join(Dir.pwd, 'gems'))
  Gem.clear_paths
  Gem.path.unshift(gem_dir)
end

gem 'extlib', '~> 0.9.9'
require 'extlib'

qtjambi_dir = "{#{ENV_JAVA['jruby.home']},#{Dir.pwd}}"
Dir.glob(ENV_JAVA['jruby.home'] / 'lib' / 'qtjambi-*.jar') { |jar| require jar }

require 'qtjruby-core'
require ARGV.shift