#!/usr/bin/env jruby
if File.exist?(lib_dir = Dir.pwd + '/lib')
  Dir.glob(lib_dir + '/lib/qtjambi-*.jar') { |jar| require jar }
end

require 'rubygems'

if File.exist?(gem_dir = File.join(Dir.pwd, 'gems'))
  Gem.clear_paths
  Gem.path.unshift(gem_dir)
end

gem 'extlib', '~> 0.9.9'
require 'extlib'

require 'qtjruby-core'
require ARGV.shift